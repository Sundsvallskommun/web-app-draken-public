{
  "name": "draken-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "env:dev:kc": "dotenv -e .env.kc",
    "env:dev:mex": "dotenv -e .env.mex",
    "env:dev:pt": "dotenv -e .env.pt",
    "dev:ik": "dotenv -e .env.ik -- next dev",
    "dev:kc": "dotenv -e .env.kc -- next dev",
    "dev:ka": "dotenv -e .env.ka -- next dev",
    "dev:lop": "dotenv -e .env.lop -- next dev",
    "dev:mex": "dotenv -e .env.mex -- next dev",
    "dev:pt": "dotenv -e .env.pt -- next dev",
    "build:ik": "dotenv -e .env.ik -- next build",
    "build:kc": "dotenv -e .env.kc -- next build",
    "build:ka": "dotenv -e .env.ka -- next build",
    "build:lop": "dotenv -e .env.lop -- next build",
    "build:mex": "dotenv -e .env.mex -- next build",
    "build:pt": "dotenv -e .env.pt -- next build",
    "start:ik": "dotenv -e .env.ik -- dotenv next start",
    "start:kc": "dotenv -e .env.kc -- dotenv next start",
    "start:ka": "dotenv -e .env.ka -- dotenv next start",
    "start:lop": "dotenv -e .env.lop -- dotenv next start",
    "start:mex": "dotenv -e .env.mex -- dotenv next start",
    "start:pt": "dotenv -e .env.pt -- dotenv next start",
    "generate:contracts:lop": "dotenv -e .env.lop npx tsx ./src/swagger-typescript-api.ts",
    "generate:contracts:kc": "dotenv -e .env.kc npx tsx ./src/swagger-typescript-api.ts",
    "generate:contracts:ka": "dotenv -e .env.ka npx tsx ./src/swagger-typescript-api.ts",
    "generate:contracts:mex": "dotenv -e .env.mex npx tsx ./src/swagger-typescript-api.ts",
    "lint": "next lint --no-cache",
    "type-check": "tsc --pretty --noEmit",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "pre-cypress": "cross-env ELECTRON_EXTRA_LAUNCH_ARGS=--disable-gpu-sandbox",
    "cypress:ik": "dotenv -e .env.ik -- cross-env ELECTRON_EXTRA_LAUNCH_ARGS=--disable-gpu-sandbox cypress open",
    "cypress:kc": "dotenv -e .env.kc -- cross-env ELECTRON_EXTRA_LAUNCH_ARGS=--disable-gpu-sandbox cypress open",
    "cypress:lop": "dotenv -e .env.lop -- cross-env ELECTRON_EXTRA_LAUNCH_ARGS=--disable-gpu-sandbox cypress open",
    "cypress:mex": "dotenv -e .env.mex -- cross-env ELECTRON_EXTRA_LAUNCH_ARGS=--disable-gpu-sandbox cypress open",
    "cypress:pt": "dotenv -e .env.pt -- cross-env ELECTRON_EXTRA_LAUNCH_ARGS=--disable-gpu-sandbox cypress open",
    "cypress:headless:ik": "dotenv -e .env.ik -- cross-env ELECTRON_EXTRA_LAUNCH_ARGS=--disable-gpu-sandbox cypress run",
    "cypress:headless:kc": "dotenv -e .env.kc -- cross-env ELECTRON_EXTRA_LAUNCH_ARGS=--disable-gpu-sandbox cypress run",
    "cypress:headless:ka": "dotenv -e .env.ka -- cross-env ELECTRON_EXTRA_LAUNCH_ARGS=--disable-gpu-sandbox cypress run",
    "cypress:headless:lop": "dotenv -e .env.lop -- cross-env ELECTRON_EXTRA_LAUNCH_ARGS=--disable-gpu-sandbox cypress run",
    "cypress:headless:mex": "dotenv -e .env.mex -- cross-env ELECTRON_EXTRA_LAUNCH_ARGS=--disable-gpu-sandbox cypress run",
    "cypress:headless:pt": "dotenv -e .env.pt -- cross-env ELECTRON_EXTRA_LAUNCH_ARGS=--disable-gpu-sandbox cypress run",
    "test": "jest --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@headlessui/react": "^1.4.2",
    "@hookform/resolvers": "^2.8.8",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.0",
    "@mui/styles": "^5.4.4",
    "@next/bundle-analyzer": "14.1.4",
    "@sk-web-gui/core": "^3.11.0",
    "@sk-web-gui/lucide-icon": "^1.1.1",
    "@sk-web-gui/react": "^1.3.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.2",
    "axios": "^1.7.2",
    "cypress-intercept-formdata": "^0.5.2",
    "dayjs": "^1.10.8",
    "dotenv-cli": "^7.4.2",
    "envalid": "^8.0.0",
    "i18next": "^24.2.3",
    "next": "^14.2.10",
    "next-i18next": "^15.4.2",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.29.0",
    "react-i18next": "^15.4.1",
    "react-image-crop": "^10.0.7",
    "react-quill": "2.0.0",
    "sanitize-html": "^2.12.1",
    "sass": "^1.43.3",
    "sharp": "^0.32.6",
    "universal-cookie": "^4.0.4",
    "uuid": "^9.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.8",
    "@cypress/skip-test": "^2.6.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "14.0.0",
    "@types/node": "^20.3.2",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.5",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^29.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^14.1.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.43.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "prettier": "^2.4.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.1.6"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  }
}
